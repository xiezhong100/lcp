#fullLoader {
	position:relative;
}

#startPlace {
	width: 225px;
	background-color: #8CB98E;
	text-align: center;
	font-family: "新宋体";
	font-size: 14px;
	line-height: 35px;
	font-weight: bold;
	color: #FFFFFF;
	float: left;
	position: relative;
	left: 0px;

}

body {
font: 14px/150% Arial, Verdana, "宋体b8b\4f53";
color: #666;
}

a:link, a:visited {
color: #666;
text-decoration: none;
}

#nav {
	position:relative;
	background-color: #8CB98E;
	width: 1210px;
	clear：both;
}

#nav #navitems li {
	display: block;
	height: 30px;
	width: 100px;
	text-align: center;
	vertical-align: middle;
	float: left;
	list-style-type:none;
	background-color: #8CB98E;
	font-size: 14px;
	font-weight: bold;
	color: #FFFFFF;
	border-left-width: 1px;
	border-top-style: none;
	border-right-style: none;
	border-bottom-style: none;
	border-left-style: solid;
	border-left-color: #999999;
	background-position: center;
	padding-top: 5px;
	padding-right: 0px;
	padding-bottom: 0px;
	padding-left: 0px;
}

#nav #navitems li a {
	font-size: 14px;
	color: #FFFFFF;
	text-align: center;
	vertical-align: middle;
}

#nav #navitems #empty-div {
	width: 364px;
	background-color: #8CB98E;

}

a:hover {
	color: #55EE44;
	text-decoration: underline;
}

a:active {
	color: #900;
}

/*allsort*/
#fullLoader .allsort {
	float: left;
	width: 225px;
	height: auto;
	background-position: 0px -101px;
	position: absolute;
	z-index: 11;
	top:35px;
}

#fullLoader .allsort .mt {
	height: 24px;
	padding: 14px 12px 12px 16px;
	line-height: 24px;
	cursor: pointer;
	overflow: hidden;
}

#fullLoader .allsort .mt strong {
	float: left;
	font-size: 14px;
	color: #630;
}

#fullLoader .allsort .mt .extra {
	float: right;
	overflow: hidden;
	width: 22px;
	height: 22px;
	background-position: -214px -52px;
}

#fullLoader .allsort .mc {
	display: none;
	position: absolute;
	top: 0px;
	overflow: visible;
	width: 218px;
	padding: 0 3px 0;
	border: solid #7FAD5D;
	border-width: 0 1px 1px;
	background: #FEF8EF;
	-moz-border-radius: 0 0 5px 5px;
	-webkit-border-radius: 0 0 5px 5px;
}

#fullLoader .allsort .item {
	width: 203px;
	height: 26px;
	border-top: 1px solid #FDE6D2;
}

#fullLoader .allsort .fore {
	border-top: none;
}

#fullLoader .allsort span {
	display: block;
	width: 203px;
	position: relative;
	z-index: 1;
}

#fullLoader .allsort h3 {
	font-size: 14px;
	width: 170px;
	height: 30px;
	padding-left: 20px;
	border: solid #FEF8EF;
	border-width: 1px 0 1px 1px;
	background-position: -241px -57px;
	font-weight: normal;
}

#fullLoader .allsort h3 a:link,.allsort h3 a:visited {
	display: block;
	height: 30px;
	line-height: 30px;
	color: #333;
}

#fullLoader .allsort h3 a:hover,.allsort h3 a:active {
	color: #1B578A;
}

#fullLoader .allsort s {
	display: block;
	position: absolute;
	top: 10px;
	left: 182px;
	width: 13px;
	height: 13px;
	background-position: -218px -106px;
}

#fullLoader .allsort .item .i-mc {
	display: none;
	position: absolute;
	left: 193px;
	top: 0;
	width: 550px;
	border: 1px solid #7FAD5D;
	background: #FFF9EF;
	overflow: hidden;
}

#fullLoader .allsort .item dt {
	padding: 3px 6px 0 0;
	font-weight: bold;
}

#fullLoader .allsort .item dd {
	padding: 3px 0 0;
	overflow: hidden;
	zoom: 1;
}

#fullLoader .allsort .subitem {
	float: left;
	width: 464px;
	min-height: 400px;
	padding: 0 4px 0 8px;
}

#fullLoader .allsort .subitem dl {
	border-top: 1px solid #FFEFD7;
	padding: 6px 0;
	overflow: hidden;
	zoom: 1;
}

#fullLoader .allsort .subitem .fore {
	border-top: none;
}

#fullLoader .allsort .subitem dt {
	float: left;
	width: 54px;
	line-height: 22px;
	text-align: right;
	color: #c00;
}

#fullLoader .allsort .subitem dd {
	float: left;
	width: 402px;
}

#fullLoader .allsort .subitem em {
	float: left;
	height: 14px;
	margin: 4px 0;
	line-height: 14px;
	padding: 0 8px;
	border-left: 1px solid #ccc;
	font-style: normal;
	white-space: nowrap;
}

#fullLoader .allsort .fr {
	background: #fff;
	width: 194px;
	padding: 0 15px 2010px 15px;
	margin-bottom: -2000px;
	float: right;
}

#fullLoader .allsort .fr dl {
	padding-bottom: 0;
}

#fullLoader .allsort .mc .extra {
	padding: 7px 8px;
	background: #FDF1DE;
	border-top: 1px solid #FDE6D2;
}

#fullLoader .allsorthover {
	background-position: 0 -50px;
}

#fullLoader .allsorthover .mt .extra {
	background-position: -204px -75px;
}

#fullLoader .allsorthover .mc {
	display: block;
}

#fullLoader .allsort .hover span {
	z-index: 13;
	width: 160px;
}

#fullLoader .allsort .hover h3 {
	font-size: 14px;
	border: solid #7FAD5D;
	border-width: 1px 0 1px 1px;
	overflow: hidden;
	background: url(images/nav-sort.gif) #FFF9EF no-repeat -241px -57px;
	font-weight: bold;
}

#fullLoader .allsort .hover s {
	display: none;
}

#fullLoader .allsort .hover .i-mc {
	display: block;
	z-index: 12;
}

* html .allsort .item dd {
	padding-bottom: 6px;
}

* html .allsort .subitem {
	height: 400px;
}

#fullLoader .allsort .close {
	position: absolute;
	top: 6px;
	left: 674px;
	z-index: 14;
	width: 19px;
	height: 19px;
	background-position: -216px -125px;
	cursor: pointer;
}