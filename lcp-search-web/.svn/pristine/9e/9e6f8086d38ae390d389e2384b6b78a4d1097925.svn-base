/*页面总体样式控制*/
body {
    background: none repeat scroll 0 0 #FFFFFF;
    color: #333333;
    font: 12px/150% Arial,Verdana,"宋体";
}
/*页面总体样式控制结束*/
/**/
html, body, div, span, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, address, big, cite, code, del, em, font, img, ins, small, strong, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend {
    margin: 0;
    padding: 0;
}

#category-select {
	clear:both;
}

a:link, a:visited {
    color: #333333;
    text-decoration: none;
}

a:hover{
	color:#CC0000;
	text-decoration: underline;
}

.mt, .smt {
    cursor: default;
}

.mt h1, .mt h2 {
    font-family: "宋体";
    font-weight: bold;
}

h1 {
    font: 20px "microsoft yahei","宋体";
}

img {
    vertical-align: middle;
}

.clr {
    clear: both;
    display: block;
    font-size: 0;
    height: 0;
    line-height: 0;
    overflow: hidden;
}



.lh li, .lh dl, .lh .item {
    float: left;
}


.w {
    width: 1210px;
}

/*结束*/


/*回到顶部*/
#toppanel {
    position: relative;
    z-index: 1000;
    width: 1210px;
    margin: 0 auto;
    width: 990px;
}	
#sidepanel {
    bottom: 0;
    position: fixed;
    right: 0;
    z-index: 1000;
}
.hide {
    display: none;
}

#sidepanel a:link, #sidepanel a:visited {
    background-position: 0 -606px;
}
#sidepanel a {
    display: block;
    height: 66px;
    line-height: 14px;
    margin: 5px 0;
    padding: 28px 4px 0;
    position: relative;
    text-align: center;
    text-decoration: none;
    width: 17px;
}
#sidepanel a, #sidepanel b {
    background-image: url("http://misc.360buyimg.com/lib/skin/2012/i/20111221C.png");
    background-repeat: no-repeat;
}



/*结束*/
	
	
/*顶部标签栏*/
#shortcut {
    line-height: 30px;
    padding-bottom: 1px;
    width: 100%;
}


#shortcut, #shortcut .w, #shortcut .wleft b, #shortcut .menu b {
    background-image: url("../images/20111221C.png");
    background-repeat: no-repeat;
}


/*宽度及居中*/
#shortcut .w{
	width: 1210px;
	margin: 0 auto;
}


#shortcut, #shortcut .w {
    background-color: #EFEFEF;
    background-repeat: repeat-x;
    height: 30px;
}

.wleft{
	overflow:hidden;
	float:left;
}


ul {
    list-style: none outside none;
}


#shortcut .wleft .fore1 {
    background: none repeat scroll 0 0 transparent;
    padding-left: 15px;
}
#shortcut li {
    background-position: -230px -320px;
    height: 27px;
    padding: 0 9px;
    background-image: url("../images/20120112B.png");
    background-repeat: no-repeat;
}


.ld {
    position: relative;
}


#shortcut .wleft span {
    display: none;
}

.wleft li{
	float:left;
}


/*收藏小图标*/
#shortcut .w .wleft b{
	background-position: -57px -289px;
    cursor: pointer;
    height: 10px;
    left: 1px;
    top: 10px;
    width: 10px;
    
    background-image: url("../images/20111221C.png");
    background-repeat: no-repeat;
    
    overflow: hidden;
    position: absolute;
}




/*顶部右侧*/
.wright{
	height: 30px;
    overflow: visible;
    float:right;
    overflow: hidden;
}

.wright li{
	float:left;
}

#shortcut .wright .fore1 {
    background: none repeat scroll 0 0 transparent;
}

#loginbar a {
    margin-left: 5px;
}

#shortcut .wright .menu {
    padding: 3px 0 0;
    position: relative;
    width: 81px;
    z-index: 10;
}

#shortcut .menu dl {
    padding: 0 6px;
    width: 67px;
     float: left;
}
#shortcut .menu dt {
    cursor: default;
    height: 24px;
    line-height: 24px;
    padding: 0 10px 1px 6px;
}

#shortcut .menu b {
    background-position: -35px -281px;
    height: 4px;
    right: 3px;
    top: 10px;
    width: 7px;
    /*background-image: url("../images/20111221C.png");*/
    /*background-repeat: no-repeat;*/
}

#shortcut .menu dd {
    display: none;
    line-height: 21px;
    padding: 4px 0 4px 5px;
}

.ld s, .ld b, .ld .block {
    overflow: hidden;
    position: absolute;
}
/*顶部右侧结束*/


/*顶部标签栏结束*/
	
	
/*头部*/
#header{
	height:60px;
	padding:15px 0;
}


	
